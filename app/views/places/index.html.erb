<h1>Listing places</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Pcode</th>
    <th>Geometry</th>
    <th>Country</th>
    <th>Region</th>
    <th>City</th>
  </tr>

<% for place in @places %>
  <tr>
    <td><%=h place.name %></td>
    <td><%=h place.pcode %></td>
    <td><%=h place.country %></td>
    <td><%=h place.region %></td>
    <td><%=h place.city %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New place', new_place_path %>
